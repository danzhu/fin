    y Int

def distance(v Vec) Int
    v.x * v.x + v.y * v.y

def print(v Vec)
    print(v.x)
    print(v.y)

def test()
    let i = 1
    let j := i

    let v Vec
    v.x = 3
    v.y = 4
    if v.x < 0
        -distance(v) + 1
    else
        i += distance(v)
        v.y + j
    .print()

test()
