struct Vec
    x Int
    y Int

def distance(v Vec) Int
    v.x * v.x + v.y * v.y

def print(v Vec)
    print(v.x)
    print(v.y)

def test(useVec Bool)
    if useVec
        let v Vec
        v.x = 3
        v.y = 4
        v.x = v.distance()
        v.x
    else
        let i Int
        i = 5
        i
    .print()

test(FALSE)
test(TRUE)
